{
  "name": "simp-upstart",
  "version": "6.0.2",
  "author": "SIMP Team",
  "summary": "A SIMP puppet module for managing upstart",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-upstart",
  "project_page": "https://github.com/simp/pupmod-simp-upstart",
